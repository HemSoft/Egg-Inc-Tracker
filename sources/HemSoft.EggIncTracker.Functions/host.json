{
    "version": "2.0",
    "logging": {
        "logLevel": {
            "Default": "Information",
            "Host": "Warning",
            "Function": "Information",
            "Azure.Core": "Warning",
            "Azure.Core.Http": "Error",
            "Azure.Identity": "Warning",
            "System.Net.Http.HttpClient": "Warning",
            "Microsoft.Azure.WebJobs": "Warning",
            "Microsoft.Azure.WebJobs.Host": "Warning",
            "Microsoft.Azure.WebJobs.Script": "Warning",
            "Microsoft.Azure.Functions.Worker": "Warning",
            "Microsoft": "Warning"
        },
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            },
            "enableLiveMetricsFilters": true
        }
    }
}